! special
*.foreground:   #c5c8c6
*.background:   #1d1d1d
*.cursorColor:  #c5c8c6

! black
*.color0:       #1d1d1d
*.color8:       #292929

! red
*.color1:       #aa2244
*.color9:       #d65877

! green
*.color2:       #167b43
*.color10:      #22bb66

! yellow
*.color3:       #1f5f61
*.color11:      #2fb7bb

! blue
*.color4:       #1e4780
*.color12:      #4f8fcf

! magenta
*.color5:       #75278f
*.color13:      #aa6abb

! cyan
*.color6:       #00aa82
*.color14:      #2daa8d

! white
*.color7:       #6d7680
*.color15:      #c5c8c6

#define PL_FONT             HeavyData Nerd Font
#define FSM_FONT            FantasqueSansMono Nerd Font


! Fonts and Encoding
XTerm*utf8: True
XTerm*locale: True
XTerm*utf8Title: True
XTerm*preeditType: Root
XTerm*xftAntialias: true
XTerm*.faceName: FSM_FONT:size=13
XTerm*renderFont: True

!                  'background', 'border', 'separator'
 rofi.color-window: #f01d1d1d,      #f01d1d1d, #22bb66

rofi.padding: 30
rofi.terminal: gnome-terminal
rofi.combi-hide-mode-prefix: true
rofi.window-format:{w}         {c}            {t}
rofi.sidebar-mode: true
rofi.separator-style: solid
